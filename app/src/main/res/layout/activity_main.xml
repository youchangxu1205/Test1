    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="cn.jldt.ms.test.MainActivity">

    <Button
        android:id="@+id/expandable_btn"
        android:text="ExpandableListViewActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/handle_btn"
        android:text="HandleActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <Button
        android:id="@+id/card_view_btn"
        android:text="CardViewActivity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</LinearLayout>
